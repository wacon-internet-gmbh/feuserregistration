                <source>Complete your registration on %s</source>
            </trans-unit>
            <trans-unit id="register.mail.doi.text.salutation" resname="register.mail.doi.text.salutation">
                <source>Dear Sir or Maddam,</source>
            </trans-unit>
            <trans-unit id="register.mail.doi.text.1" resname="register.mail.doi.text.1">
